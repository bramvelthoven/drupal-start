# Ignore directories generated by Composer
/bin
/drush/contrib/
/vendor/
/web/*
!/web/*/
/web/core/
/web/libraries/
/web/modules/contrib/
/web/themes/contrib/
/web/profiles/contrib/

# But keep possibly customised htaccess.
!/web/.htaccess

# Ignore sensitive information
settings.local.php

# Ignore Drupal's file directory
/web/sites/*/files/
/private

# ignore text files
*.txt
# But keep possibly customised robots.txt.
!/web/robots.txt
# And also keep solr config txt files.
!/config/solr/*/*.txt
!/.ddev/solr/*/*.txt

# Ignore file types
*.tar
*.gz
*.zip
*.sql
*.avi
.sass-cache
.DS_Store

# Ignore files generated by PhpStorm
/.idea/
/.project

LICENSE
\.editorconfig
\.env\.example

# Ignore docker stuff
/.solr-data/
/.env
/docker-compose.yml
/docker-sync.yml
/.docker-sync/
.docker-sync/

#Ignore theme building files.
node_modules
.gitattributes

#Backstop data
tests/*/backstop_data/bitmaps_reference
tests/*/backstop_data/bitmaps_test
tests/*/backstop_data/html_report
tests/*/backstop_data/ci_report

#Pa11y data
tests/*/data

#Ignore the default grumphp config file so the grumphp.yml.dist gets used
grumphp.yml
/tests/sitespeed.io/sitespeed-result/

tests/behat/chromedriver
tests/behat/results/
tests/behat/behat-local.yml
